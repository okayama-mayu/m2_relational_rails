<h1> Menu Sections </h1>
<% @sections.each_with_index do |section, index| %>
    <div id="section-<%= index %>">
    <p><b><%= section.name %></b><span id="section-<%= index %>b">  Created at: <%= section.created_at %></span></div>
<% end %>

<%= link_to "Create New Menu Section", "/sections/new"%>